@prefix nobias: <https://nobias-project.eu/> .
@base <https://nobias-project.eu/> .


  
<BiasEventMapping>
  a rr:TriplesMap;

  rml:logicalSource [
	rml:source "/git/SDM-RDFizer/mappings/input/XXX.csv";
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
  rr:template "https://nobias-project.eu/BiasEvent/BiasEvent1-{NewsID}";
  rr:class nobias:BiasEvent
  ];  

  rr:predicateObjectMap [
	 rr:predicate nobias:hasScore;
	  rr:objectMap [
		  rml:reference "Overrepresented"
	  ]
  ]; 

  
  rr:predicateObjectMap [
	  rr:predicate nobias:hasSourceParticipant;
	  rr:objectMap [
		  rr:template "https://nobias-project.eu/News/{NewsID}";
	  ]
  ];  

 
  rr:predicateObjectMap [
     rr:predicate nobias:hasDetectionMethod ; 
     rr:objectMap [ 
         rr:constant nobias:OverrepresentationDetection  
            ]
   ].
  
  
  





