    </owl:Class>
    


    <!-- http://www.w3.org/ns/dqv#Metric -->

    <owl:Class rdf:about="http://www.w3.org/ns/dqv#Metric">
        <rdfs:comment>Metric is a standard to measure a quality dimension. An observation(instance of dqv:QualityMeasurement) assigns a value in a given unit to a Metric.</rdfs:comment>
        <rdfs:label xml:lang="en">QualityMetric</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/dqv#QualityAnnotation -->

    <owl:Class rdf:about="http://www.w3.org/ns/dqv#QualityAnnotation">
        <rdfs:comment>QualityAnnotation is a quality annotations, including ratings, quality certificates or feedback that can be associated to datasets</rdfs:comment>
        <rdfs:label>QualityAnnotation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/dqv#QualityCertificate -->

    <owl:Class rdf:about="http://www.w3.org/ns/dqv#QualityCertificate">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dqv#QualityAnnotation"/>
        <rdfs:comment>QualityCertificate is subclass of QualityAnnotation. It associates a dataset to a document that	certifies the data quality according to a set of quality assessment rules</rdfs:comment>
        <rdfs:label>QualityCertificate</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/dqv#QualityMeasurement -->

    <owl:Class rdf:about="http://www.w3.org/ns/dqv#QualityMeasurement">
        <rdfs:comment>QualityMeasurement is the evaluation of a given dataset a specific quality metric.</rdfs:comment>
        <rdfs:label>QualityMeasurement</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/dqv#QualityPolicy -->

    <owl:Class rdf:about="http://www.w3.org/ns/dqv#QualityPolicy">
        <rdfs:comment xml:lang="en">QualityPolicy is a policy or agreement that is chiefly governed by data quality concerns</rdfs:comment>
        <rdfs:label xml:lang="en">QualityPolicy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#Algorithm -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#Algorithm">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#InformationEntity"/>
        <rdfs:comment xml:lang="en">An optimization algorithm in machine learning problems minimizes a loss or cost function by systematically choosing input values from within an allowed set and computing the value of the function.</rdfs:comment>
        <rdfs:label xml:lang="en">Algorithm</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#EvaluationMeasure -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#EvaluationMeasure">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#InformationEntity"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">russo</dc:creator>
        <rdfs:comment xml:lang="en">EvaluationMeasure is a measure to assess the performance of the model generated by the process that realizes the task. Examples are predictive accuracy or f-measure.</rdfs:comment>
        <rdfs:label xml:lang="en">EvaluationMeasure</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#EvaluationProcedure -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#EvaluationProcedure">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#InformationEntity"/>
        <rdfs:comment xml:lang="en">EvaluationProcedure is a technique to evaluate machine learning models. Examples are cross-validation and leave-one-out.</rdfs:comment>
        <rdfs:label>EvaluationProcedure</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#EvaluationSpecification -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#EvaluationSpecification">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#InformationEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/mls#EvaluationMeasure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/mls#EvaluationProcedure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#specifiedBy"/>
                <owl:someValuesFrom rdf:resource="https://nobias-project.eu/Iteration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">russo</dc:creator>
        <rdfs:comment xml:lang="en">EvaluationSpecification is a specification of an evaluation measure and an evaluation procedure to assess the performance of the process that realizes the task.</rdfs:comment>
        <rdfs:label xml:lang="en">EvaluationSpecification</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#Experiment -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#Experiment">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#Process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/mls#Run"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The training process of fake news detection model consists of several experiments that are subclasses of the Process and they are part of a Study(research project). Each Experiment is done for a specific goal decided by the study it is part of.</rdfs:comment>
        <rdfs:label>Experiment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#Implementation -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#Implementation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#InformationEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/implements"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/mls#Algorithm"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Implementation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#InformationEntity -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#InformationEntity">
        <rdfs:label xml:lang="en">InformationEntity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#Model -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#Model">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#InformationEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/hasOutput"/>
                <owl:someValuesFrom rdf:resource="https://www.openriskmanual.org/ns/doam#Output"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Model is a generalization of a set of training data able to predict values for unseen instances. It is an output from an execution of a data mining algorithm implementation. Models have a dual nature. They can be treated as data structures and as such represented, stored and manipulated. On the other hand, they act as functions and are executed, taking as input data examples and giving as output the result of applying the function to a data example. Models can also be divided into global or local ones. A global model has global coverage of a data set, i.e., it generalizes the whole data set. A local model, such as a pattern set, is a set of local hypotheses, i.e. each applies to a limited region of the data set.</rdfs:comment>
        <rdfs:label xml:lang="en">Model</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#ModelEvaluation -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#ModelEvaluation">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#InformationEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#specifiedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/mls#EvaluationMeasure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/hasDocumentationType"/>
                <owl:someValuesFrom rdf:resource="https://nobias-project.eu/LearningDocumentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">ModelEvaluation is a setting of a value of the performance measure specified by the evaluation specification. It connects a measure specification with its value.</rdfs:comment>
        <rdfs:label xml:lang="en">ModelEvaluation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#Process -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#Process">
        <rdfs:comment xml:lang="en">This class represents  the process  followed in a classification problem using a particular machine learning model.</rdfs:comment>
        <rdfs:label>Process</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#Run -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#Run">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#Process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#achieves"/>
                <owl:someValuesFrom rdf:resource="https://nobias-project.eu/Iteration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#hasInput"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#realizes"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/mls#Algorithm"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/hasOutput"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/mls#Model"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/hasOutput"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/mls#ModelEvaluation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Run is an execution of an implementation on a machine (computer). It is limited in time (has a start and end point), can be successful or failed.</rdfs:comment>
        <rdfs:label>Run</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#Study -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#Study">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#Process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/mls#Experiment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Study is a collection of runs that belong together to do some kind of analysis on its results. This analysis can be general or very specific (e.g. a hypothesis test). Can be linked to files, data, that belong to it.</rdfs:comment>
        <rdfs:label>Study</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/mls#Task -->

    <owl:Class rdf:about="http://www.w3.org/ns/mls#Task">
        <owl:equivalentClass rdf:resource="https://nobias-project.eu/Iteration"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/mls#InformationEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#definedOn"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/mls#defines"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/mls#EvaluationSpecification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Task</rdfs:label>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Person -->

    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person">
        <rdfs:comment xml:lang="en">Represents the creator of the dataset.</rdfs:comment>
        <rdfs:label xml:lang="en">Creator</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/ActivityBias -->

    <owl:Class rdf:about="https://nobias-project.eu/ActivityBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/InteractionBias"/>
        <dc:citation xml:lang="en">Baeza-Yates, R. 2020. Biases on Social Media Data: (Keynote). In Companion Proceedings of the Web Conference 2020 (WWW ’20Companion), April 20–24, 2020, Taipei, Taiwan.ACM, New York, NY, USA,2 pages. 
https://doi.org/10.1145/3366424.3383564</dc:citation>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">ActivityBias</rdfs:label>
        <skos:definition xml:lang="en">This refers to how not all people are active all the time in social media (90% are lurkers), and how a select few are responsible for creating more than 50% of content.</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/AnnotatorBias -->

    <owl:Class rdf:about="https://nobias-project.eu/AnnotatorBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/StatisticalBias"/>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">AnnotatorBias</rdfs:label>
        <skos:definition xml:lang="en">Annotator bias is associated to data labelled or/and annotated by humans, and it refers to how every annotator has its own habit to label the data, as well as motivations (i.e. financial).</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/Argument -->

    <owl:Class rdf:about="https://nobias-project.eu/Argument">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/hasDocumentationType"/>
                <owl:someValuesFrom rdf:resource="https://nobias-project.eu/LearningDocumentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An argument is the term of an atom that is linked with a predicate. The amount of arguments per atom is equal to the predicate’s arity.</rdfs:comment>
        <rdfs:label xml:lang="en">Argument</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/Atom -->

    <owl:Class rdf:about="https://nobias-project.eu/Atom">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/hasDocumentationType"/>
                <owl:someValuesFrom rdf:resource="https://nobias-project.eu/LearningDocumentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An atom is a predicate combined with a sequence of terms of length equal to the predicate’s arity. This sequence is called the atom’s arguments. Two Atoms are equal if their Predicate and arguments are equal.</rdfs:comment>
        <rdfs:label xml:lang="en">Atom</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/AtomArgument -->

    <owl:Class rdf:about="https://nobias-project.eu/AtomArgument">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/hasDocumentationType"/>
                <owl:someValuesFrom rdf:resource="https://nobias-project.eu/LearningDocumentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">russo</dc:creator>
        <rdfs:label xml:lang="en">AtomArgument</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/BeliefBias -->

    <owl:Class rdf:about="https://nobias-project.eu/BeliefBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/CognitiveBias"/>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">BeliefBias</rdfs:label>
        <skos:definition xml:lang="en">Belief bias is the tendency to judge the strength of arguments based on the plausibility of their conclusion rather than how strongly they support that conclusion. A person is more likely to accept an argument that supports a conclusion that aligns with their values, beliefs and prior knowledge, while rejecting counter arguments to the conclusion.</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/BiasDetectionMethod -->

    <owl:Class rdf:about="https://nobias-project.eu/BiasDetectionMethod">
        <rdfs:comment xml:lang="en">Bias Detection Method represents the method that was used to detect bias. For example, statistical analysis, expert assessment, quality metrics, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">BiasDetectionMethod</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/BiasDocumentation -->

    <owl:Class rdf:about="https://nobias-project.eu/BiasDocumentation">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/DocumentationType"/>
        <rdfs:label xml:lang="en">BiasDocumentation</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/BiasType -->

    <owl:Class rdf:about="https://nobias-project.eu/BiasType">
        <dc:citation xml:lang="en">Pitoura, E. 2020. Social-minded Measures of Data Quality: Fairness, Diversity, and Lack of Bias. J. Data and Information Quality 12, 3, Article 12 (July 2020), 8 pages.</dc:citation>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">BiasType</rdfs:label>
        <skos:definition xml:lang="en">Bias is an inclination, or prejudice for, or against one person, or group, especially in a way considered to be unfair and “a concentration on, or interest in one particular area, or subject.”</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/ClassificationFramework -->

    <owl:Class rdf:about="https://nobias-project.eu/ClassificationFramework">
        <rdfs:comment xml:lang="en">Framework  used to solve the classification  problem, for instance identify which of a set of categories (sub-populations) an observation (or observations) belongs to.</rdfs:comment>
        <rdfs:label xml:lang="en">ClassificationFramework</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/CognitiveBias -->

    <owl:Class rdf:about="https://nobias-project.eu/CognitiveBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/BiasType"/>
        <dc:citation xml:lang="en">Manoogian J, Benson B (2017) Cognitive bias codex.https://betterhumans.pub/cognitive-bias-cheat-sheet-55a472476b18</dc:citation>
        <dc:citation xml:lang="en">https://en.wikipedia.org/wiki/Cognitive_bias</dc:citation>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">CognitiveBias</rdfs:label>
        <skos:comment xml:lang="en">Cognitive biases can sometimes lead to perceptual distortion, inaccurate judgment, illogical interpretation, or what is broadly called irrationality.</skos:comment>
        <skos:definition xml:lang="en">Cognitive biases are systematic patterns of deviation from norm and/or rationality in judgment.</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/ConfirmationBias -->

    <owl:Class rdf:about="https://nobias-project.eu/ConfirmationBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/CognitiveBias"/>
        <dc:citation xml:lang="en">Farkas, J. &amp; Schou, J. (2018) Fake News as a Floating Signifier: Hegemony, Antagonism and the Politics of Falsehood, Javnost - The Public, 25:3, 298-314, DOI:
10.1080/13183222.2018.1463047</dc:citation>
        <dc:citation xml:lang="en">Manoogian J, Benson B (2017) Cognitive bias codex.https://betterhumans.pub/cognitive-bias-cheat-sheet-55a472476b18</dc:citation>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">ConfirmationBias</rdfs:label>
        <skos:comment xml:lang="en">Disconfirmation bias or motivated skepticism works the same concept but in opposite direction and is what gives way to polarization within the context of disinformation, fake news and political convictions.</skos:comment>
        <skos:definition xml:lang="en">Confirmation bias is the tendency to search for, interpret, favor, and recall information in a way that confirms or supports one&apos;s prior beliefs or values.</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/CoverageBias -->

    <owl:Class rdf:about="https://nobias-project.eu/CoverageBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/StatisticalBias"/>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">CoverageBias</rdfs:label>
        <skos:comment xml:lang="en">Upon data acquisition, data scientist or machine learning engineer must ensure  that there are enough entries in the datasets for each object category. Concept closely linked to diversity.</skos:comment>
        <skos:definition xml:lang="en">Coverage bias in survey research means that a research sample is not representative and that a number of people in the population have zero chance of being included in the sample.</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/DataCleaning -->

    <owl:Class rdf:about="https://nobias-project.eu/DataCleaning">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/Pre-processing"/>
        <rdfs:comment>method used for data cleaning</rdfs:comment>
        <rdfs:label xml:lang="en">DataCleaning</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/DataIntegration -->

    <owl:Class rdf:about="https://nobias-project.eu/DataIntegration">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/Pre-processing"/>
        <rdfs:comment>method used for data integration</rdfs:comment>
        <rdfs:label xml:lang="en">DataIntegration</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/DataTransformation -->

    <owl:Class rdf:about="https://nobias-project.eu/DataTransformation">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/Pre-processing"/>
        <rdfs:comment>method used for data transformation</rdfs:comment>
        <rdfs:label xml:lang="en">DataTransformation</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/DatarReduction -->

    <owl:Class rdf:about="https://nobias-project.eu/DatarReduction">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/Pre-processing"/>
        <rdfs:comment>method used for data reduction</rdfs:comment>
        <rdfs:label xml:lang="en">DataReduction</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/DatasetDocumentation -->

    <owl:Class rdf:about="https://nobias-project.eu/DatasetDocumentation">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/DocumentationType"/>
        <rdfs:label xml:lang="en">DatasetDocumentation</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/DocumentationType -->

    <owl:Class rdf:about="https://nobias-project.eu/DocumentationType">
        <rdfs:label xml:lang="en">DocumentationType</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/ExpertAssessment -->

    <owl:Class rdf:about="https://nobias-project.eu/ExpertAssessment">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/BiasDetectionMethod"/>
    </owl:Class>
    


    <!-- https://nobias-project.eu/ExposureBias -->

    <owl:Class rdf:about="https://nobias-project.eu/ExposureBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/FunctionalBias"/>
        <dc:citation xml:lang="en">Baeza-Yates, R. 2019. Bias in Search and Recommender Systems: (Keynote).</dc:citation>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">ExposureBias</rdfs:label>
        <skos:comment xml:lang="en">Another related term &apos;echo chamber&apos;, which refers to situations in which beliefs are amplified or reinforced by communication and repetition inside a closed system and insulated from rebuttal.</skos:comment>
        <skos:comment xml:lang="en">Related term is &apos;Filter Bubble&apos;, which is a phenomenon that limits an individual’s exposure to a full spectrum of news and other information on the internet by algorithmically prioritizing content that matches a user’s demographic profile and online history or excluding content that does not.</skos:comment>
        <skos:comment xml:lang="en">The term filter bubble was coined by internet activist Eli Pariser in his book, &quot;The Filter Bubble: What the Internet Is Hiding from You&quot; (2011).</skos:comment>
        <skos:definition xml:lang="en">This refers to the type of content that is shown or exposed to Social Media platform users.</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/FactCheckResource -->

    <owl:Class rdf:about="https://nobias-project.eu/FactCheckResource">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/associatedTo"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/hasDocumentationType"/>
                <owl:someValuesFrom rdf:resource="https://nobias-project.eu/DatasetDocumentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Factcheck resource is a third party that assess the veracity and or credibility of news, claims, etc. as well as of news publishers.</rdfs:comment>
        <rdfs:label xml:lang="en">FactCheckResource</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/FunctionalBias -->

    <owl:Class rdf:about="https://nobias-project.eu/FunctionalBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/BiasType"/>
        <dc:citation xml:lang="en">Olteanu A, Castillo C, Diaz F and Kıcıman E (2019) Social Data: Biases, Methodological Pitfalls, and Ethical Boundaries. Front. Big Data 2:13. doi: 10.3389/fdata.2019.00013</dc:citation>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">FunctionalBias</rdfs:label>
        <skos:definition xml:lang="en">Biases that are a result of platform-specific mechanisms or affordances, that is, the possible actions within each system or environment.</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/GeographicalBias -->

    <owl:Class rdf:about="https://nobias-project.eu/GeographicalBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/StatisticalBias"/>
        <dc:citation xml:lang="en">Ricardo Baeza-Yates. 2018. Bias on the web. Commun. ACM 61, 6 (June 2018), 54–61. DOI:https://doi.org/10.1145/3209581</dc:citation>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">GeographicalBias</rdfs:label>
        <skos:definition xml:lang="en">When false assumptions are made based on locale, or failure to account for regional differences. This type of bias can manifest itself in dfifferent ways: absense of data from certain location, major locations are overrepresented, failure to detect regional disparities or singularties, among others.</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/GroundAtom -->

    <owl:Class rdf:about="https://nobias-project.eu/GroundAtom">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/Atom"/>
        <rdfs:comment xml:lang="en">An atom with only constants for arguments is called a ground atom and has a truth value in the range [0,1].</rdfs:comment>
        <rdfs:label xml:lang="en">GroundAtom</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/GroundRule -->

    <owl:Class rdf:about="https://nobias-project.eu/GroundRule">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/Rule"/>
        <rdfs:comment>Represented as a logical disconjunction of ground atoms.</rdfs:comment>
        <rdfs:label xml:lang="en">GroundRule</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/GroundRuleGroundAtom -->

    <owl:Class rdf:about="https://nobias-project.eu/GroundRuleGroundAtom">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/hasDocumentationType"/>
                <owl:someValuesFrom rdf:resource="https://nobias-project.eu/LearningDocumentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">russo</dc:creator>
        <rdfs:label xml:lang="en">GroundRuleGroundAtom</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/IngroupBias -->

    <owl:Class rdf:about="https://nobias-project.eu/IngroupBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/CognitiveBias"/>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">IngroupBias</rdfs:label>
        <skos:definition xml:lang="en">A bias in which people tend to favor people who exist in similar groups as them. These groups could be formed by gender, race, ethnicity, favorite sports team, political convictions.</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/InstantiatedArgument -->

    <owl:Class rdf:about="https://nobias-project.eu/InstantiatedArgument">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/Argument"/>
        <rdfs:comment xml:lang="en">A constant that subsitutes an argument to a predicate.</rdfs:comment>
        <rdfs:label xml:lang="en">InstantiatedArgument</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/InteractionBias -->

    <owl:Class rdf:about="https://nobias-project.eu/InteractionBias">
        <rdfs:subClassOf rdf:resource="https://nobias-project.eu/BiasType"/>
        <dc:citation xml:lang="en">Olteanu A, Castillo C, Diaz F and Kıcıman E (2019) Social Data: Biases, Methodological Pitfalls, and Ethical Boundaries. Front. Big Data 2:13. doi: 10.3389/fdata.2019.00013</dc:citation>
        <dc:creator xml:lang="en">russoma</dc:creator>
        <rdfs:label xml:lang="en">InteractionBias</rdfs:label>
        <skos:definition xml:lang="en">Systematic distortions in user behavior across platforms or contexts, or across users represented in different datasets that arise due the user interaction with the platform.</skos:definition>
    </owl:Class>
    


    <!-- https://nobias-project.eu/Iteration -->

    <owl:Class rdf:about="https://nobias-project.eu/Iteration">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://nobias-project.eu/hasDocumentationType"/>
                <owl:someValuesFrom rdf:resource="https://nobias-project.eu/LearningDocumentation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">russo</dc:creator>
        <rdfs:label xml:lang="en">Iteration</rdfs:label>
    </owl:Class>
    


    <!-- https://nobias-project.eu/IterationRVA -->

    <owl:Class rdf:about="https://nobias-project.eu/IterationRVA">
        <rdfs:subClassOf>
