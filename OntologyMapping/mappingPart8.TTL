  a rr:TriplesMap;

  rml:logicalSource [
	rml:source "/Users/russo/git/SDM-RDFizer/mappings/input/has_performance.csv";
    rml:referenceFormulation ql:CSV
  ];
  
  rr:subjectMap [
  rr:template "https://nobias-project.eu/EvaluationSpecification/{fold_id}_{performance_id}_F1";
