  ];

  rr:subjectMap [
  rr:template "https://nobias-project.eu/EvaluationMeasure/{id}_F1";
  rr:class mls:EvaluationMeasure
  ];
  
  rr:predicateObjectMap [
	  rr:predicate nobias:f1Score;
