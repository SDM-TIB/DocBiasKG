<Run>
  a rr:TriplesMap;

  rml:logicalSource [
	rml:source "/Users/russo/git/SDM-RDFizer/mappings/input/iterations.csv";
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
  rr:template "https://nobias-project.eu/Run/{id}_{Dataset}_{Model}";
  rr:class nobias:Run
  ];
  
  rr:predicateObjectMap [
	  rr:predicate nobias:hasInput;
	  rr:objectMap [
		  rr:template "https://nobias-project.eu/Dataset/{Dataset}";
	  ]
  ];
  
  rr:predicateObjectMap [
	  rr:predicate nobias:hasOutput;
	  rr:objectMap [
		  rr:template "https://nobias-project.eu/PSLModel/{Model}";
	  ]
  ];

  rr:predicateObjectMap [
	  rr:predicate nobias:hasOutput;
	  rr:objectMap [
		  rr:template "https://nobias-project.eu/ModelEvaluation/{performance_id}";
	  ]
  ].
  
