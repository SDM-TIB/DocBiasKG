@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix mls: <http://www.w3.org/ns/mls#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix nobias: <https://nobias-project.eu/> .
@base <https://nobias-project.eu/> .


<Model>
  a rr:TriplesMap;

  rml:logicalSource [
	rml:source "/Users/russo/git/SDM-RDFizer/mappings/input/model.csv";
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
  rr:template "https://nobias-project.eu/PSLModel/{id}";
  rr:class nobias:PSLModel
  ];
  
    
  rr:predicateObjectMap [
	  rr:predicate nobias:modelName;
	  rr:objectMap [
		  rml:reference "cred_model"
	  ]
  ].

  
  
<Fold>
  a rr:TriplesMap;

  rml:logicalSource [
	rml:source "/Users/russo/git/SDM-RDFizer/mappings/input/folds.csv";
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
  rr:template "https://nobias-project.eu/CVFold/{id}";
  rr:class nobias:CVFold
  ];
  
    
  rr:predicateObjectMap [
	  rr:predicate nobias:foldNumber;
	  rr:objectMap [
		  rml:reference "fold_number"
	  ]
  ];

  rr:predicateObjectMap [
	  rr:predicate nobias:dataset;
	  rr:objectMap [
		  rml:reference "dataset"
	  ]
  ];

  rr:predicateObjectMap [
	  rr:predicate nobias:model;
	  rr:objectMap [
		  rml:reference "model"
	  ]
  ].
  

 
<Performance1>
  a rr:TriplesMap;

  rml:logicalSource [
	rml:source "/Users/russo/git/SDM-RDFizer/mappings/input/performance.csv";
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
  rr:template "https://nobias-project.eu/EvaluationMeasure/{id}_Accuracy";
  rr:class mls:EvaluationMeasure
  ];
  
  rr:predicateObjectMap [
	  rr:predicate nobias:accuracy;
	  rr:objectMap [
		  rml:reference "Accuracy"
	  ]
  ].
  

<Performance2>
  a rr:TriplesMap;

  rml:logicalSource [
