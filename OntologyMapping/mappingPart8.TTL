		  rml:reference "F1"
	  ]
  ].
  


<MappingPerformance>
  a rr:TriplesMap;

  rml:logicalSource [
	rml:source "/Users/russo/git/SDM-RDFizer/mappings/input/has_performance.csv";
    rml:referenceFormulation ql:CSV
  ];
  
  rr:subjectMap [
  rr:template "https://nobias-project.eu/ModelEvaluation/{performance_id}";
  rr:class mls:ModelEvaluation
  ].


<EvalSpec>
  a rr:TriplesMap;

  rml:logicalSource [
	rml:source "/Users/russo/git/SDM-RDFizer/mappings/input/has_performance.csv";
    rml:referenceFormulation ql:CSV
  ];
  
  rr:subjectMap [
  rr:template "https://nobias-project.eu/EvaluationSpecification/{fold_id}_{performance_id}_Accuracy";
  rr:class mls:EvaluationSpecification
  ];

  rr:predicateObjectMap [
	  rr:predicate nobias:hasPart;
	  rr:objectMap [
		  rr:template "https://nobias-project.eu/CVFold/{fold_id}";
	  ]
  ];

  rr:predicateObjectMap [
	  rr:predicate nobias:hasPart;
	  rr:objectMap [
		  rr:template "https://nobias-project.eu/EvaluationMeasure/{performance_id}_Accuracy";
	  ]
  ].


